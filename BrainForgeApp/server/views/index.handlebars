<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>React-Express Starter App</title>
  <link rel="preconnect" href="https://fonts.googleapis.com">
  <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
  <link href="https://fonts.googleapis.com/css2?family=JetBrains+Mono:ital,wght@0,100..800;1,100..800&display=swap" rel="stylesheet">
  <link rel="stylesheet" href="https://fonts.googleapis.com/css2?family=Material+Symbols+Outlined:opsz,wght,FILL,GRAD@24,400,0,0" />
  {{#if debug}}
    <script type="module">
      import RefreshRuntime from '{{assetUrl}}/static/@react-refresh'
      RefreshRuntime.injectIntoGlobalHook(window)
      window.$RefreshReg$ = () => {}
      window.$RefreshSig$ = () => (type) => type
      window.__vite_plugin_react_preamble_installed__ = true
    </script>
    <script type="module" src="{{assetUrl}}/static/@vite/client"></script>
    <script type="module" src="{{assetUrl}}/static/src/main.jsx"></script>
  {{else}}
    <link rel="stylesheet" href="/static/{{cssFile}}" />
    <script type="module" src="/static/{{jsFile}}"></script>
  {{/if}}
</head>
<body>
  <div id="root"></div>
</body>
</html>